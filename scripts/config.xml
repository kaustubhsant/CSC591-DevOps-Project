  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>sudo apt-get install python python-pip pylint -y
sudo pip install -e .
sudo pip install -r requirements-dev.txt
python ./tests/test_dependencies.py
</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <org.jenkinsci.plugins.postbuildscript.PostBuildScript plugin="postbuildscript@0.17">
      <buildSteps>
        <hudson.tasks.Shell>
          <command>mkdir htmlcov

# Run the test cases
sudo python tests/run_tests.py

# Run the coverage
coverage erase
sudo coverage run --branch tests/run_tests.py
coverage report --include=*saws/*
coverage html -d htmlcov
python -m coverage xml --include=*saws/*

# Run the static analysis tool
pylint -f parseable -d I0011,R0801 saws | tee pylint.out 

# Run custom-analysis tool
python tests/custom-analysis.py saws


</command>
        </hudson.tasks.Shell>
      </buildSteps>
      <scriptOnlyIfSuccess>true</scriptOnlyIfSuccess>
      <scriptOnlyIfFailure>false</scriptOnlyIfFailure>
      <markBuildUnstable>false</markBuildUnstable>
    </org.jenkinsci.plugins.postbuildscript.PostBuildScript>
    <hudson.plugins.cobertura.CoberturaPublisher plugin="cobertura@1.9.7">
      <coberturaReportFile>coverage.xml</coberturaReportFile>
      <onlyStable>false</onlyStable>
